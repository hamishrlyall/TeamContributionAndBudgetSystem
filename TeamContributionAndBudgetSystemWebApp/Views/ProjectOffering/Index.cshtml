                @Html.DisplayFor(modelItem => item.Project.Name)
            </td>
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { id=item.ProjectOfferingId }) |*@
                @Html.ActionLink("Set up Teams", "Details", new { id = item.ProjectOfferingId })
                @*@Html.ActionLink("Delete", "Delete", new { id=item.ProjectOfferingId })*@
            </td>
        </tr>
    }

</table>
