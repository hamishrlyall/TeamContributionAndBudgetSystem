@model List<TeamContributionAndBudgetSystemWebApp.Models.Year>

@{
   ViewBag.Title = "Index";
}

<h2>Years</h2>

<p>
   @Html.ActionLink( "Create New", "Create" )
</p>
<table class="table">
   <tr>
      <th>
         @Html.DisplayNameFor( model => model.First( ).YearValue )
      </th>
      <th></th>
   </tr>

   @foreach( var item in Model )
   {
      <tr>
         <td>
            @Html.DisplayFor( modelItem => item.YearValue )
         </td>
         <td>
            @Html.ActionLink( "Delete", "Delete", new { id = item.YearId } )
         </td>
      </tr>
   }

</table>